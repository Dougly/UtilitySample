platform :ios, '10.0'

target 'TableMe' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'


  # Pods for TableMe

  target 'TableMeTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
